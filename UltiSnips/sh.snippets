snippet / "Comment" b
# $0
endsnippet

snippet info "INFO" b
INFO "$1"
endsnippet

snippet infoo "INFO" b
INFO '$1:' $1
endsnippet

snippet echoo "echo with var" b
echo '$1:' $$1 >&2
endsnippet

snippet if "if" b
if [[ $1 ]] ; then
	$0
fi
endsnippet

snippet fun "function" b
function $1() {
	$0
}
endsnippet

snippet todo "TODO" b
# TODO: $0
endsnippet
